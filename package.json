{
  "name": "nano-id-generator",
  "displayName": "nano-id-generator",
  "description": "Nano ID generator",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nano-id-generator.generatorToClipboard",
    "onCommand:nano-id-generator.generatorToEditor"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nano-id-generator.generatorToClipboard",
        "title": "Nano ID: generator to clipboard"
      },
      {
        "command": "nano-id-generator.generatorToEditor",
        "title": "Nano ID: generator to editor"
      }
    ],
    "keybindings": [
      {
        "command": "nano-id-generator.generatorToEditor",
        "key": "ctrl+n ctrl+i",
        "mac": "cmd+n cmd+i",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Nano ID Generator",
      "properties": {
        "nano-id-generator.alphabet": {
          "type": "string",
          "default": "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ_abcdefghijklmnopqrstuvwxyz-",
          "description": "Alphabet",
          "scope": "window"
        },
        "nano-id-generator.size": {
          "type": "number",
          "default": "21",
          "description": "Characters length",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "ts-loader": "^9.2.5",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "@vscode/test-electron": "^1.6.2"
  }
}
